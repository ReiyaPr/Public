 <% @memo.each do |memo| %>
<div class="memo_title">タイトル</div>
<div class="title"> <%= escape(memo['title']) %></div>
<div class="article_title">内容</div>
<div class="article"><%= escape(memo['article'])%></div>
<button><a href="/memos/<%= memo['id'] %>/edit" type="submit">編集</a></button>
<form action='/memos/<%= memo['id'] %>' method="post">
   <input type="hidden" name="_method" value="DELETE">
   <button type="submit" value="削除">削除</button>
</form>
<% end %>
